<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="525.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AjouterVehicule.AjouterVehicule">
    <children>
        <Pane prefHeight="65.0" prefWidth="500.0" style="-fx-background-color: #406C87;">
            <children>
                <Label alignment="CENTER" layoutX="67.0" layoutY="15.0" prefHeight="29.0" prefWidth="366.0" stylesheets="@../Css/textStyle.css" text="Ajouter une nouvelle Vehicule " textFill="WHITE" />
            </children>
        </Pane>
          <VBox layoutY="64.0" prefHeight="462.0" prefWidth="500.0">
         <children>
            <HBox prefHeight="47.0" prefWidth="500.0">
               <padding>
                  <Insets top="20.0" />
               </padding>
               <children>
                  <Label styleClass="text" stylesheets="@../Css/MinitextStyle.css" text="Veuillez Remplir les informations de la Voiture :" textFill="#63b4ff">
                     <HBox.margin>
                        <Insets left="20.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="10.0" prefHeight="47.0" prefWidth="500.0">
               <padding>
                  <Insets top="20.0" />
               </padding>
               <children>
                  <Label prefWidth="160.0" stylesheets="@../Css/MinitextStyle.css" text="Matricule de la voiture :">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="matriculeTextField" alignment="CENTER" prefWidth="180.0">
                     <HBox.margin>
                        <Insets left="60.0" />
                     </HBox.margin>
                     <stylesheets>
                        <URL value="@../Css/textinputstyle.css" />
                        <URL value="@../Css/MinitextStyle.css" />
                     </stylesheets>
                     <styleClass>
                        <String fx:value="text" />
                        <String fx:value="text-field" />
                     </styleClass>
                  </TextField>
               </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="57.0" prefHeight="47.0" prefWidth="500.0">
               <padding>
                  <Insets top="20.0" />
               </padding>
               <children>
                  <Label prefWidth="160.0" stylesheets="@../Css/MinitextStyle.css" text="Nombre de place :">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="nombrePlaceTextField" alignment="CENTER" prefWidth="180.0">
                     <stylesheets>
                        <URL value="@../Css/textinputstyle.css" />
                        <URL value="@../Css/MinitextStyle.css" />
                     </stylesheets>
                     <styleClass>
                        <String fx:value="text" />
                        <String fx:value="text-field" />
                     </styleClass>
                     <HBox.margin>
                        <Insets left="60.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="104.0" prefHeight="47.0" prefWidth="500.0">
               <padding>
                  <Insets top="20.0" />
               </padding>
               <children>
                  <Label prefWidth="160.0" stylesheets="@../Css/MinitextStyle.css" text="Type de carburant :">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <JFXComboBox fx:id="carburantComboBox" focusColor="#63b4ff" prefHeight="25.0" prefWidth="180.0" promptText="Carburant" style="-jfx-label-float: true;" unFocusColor="#cdcdcd">
                     <HBox.margin>
                        <Insets left="60.0" />
                     </HBox.margin>
                     <stylesheets>
                        <URL value="@../Css/combo.css" />
                        <URL value="@../Css/MinitextStyle.css" />
                     </stylesheets>
                  </JFXComboBox>
                  <JFXButton fx:id="ajouterCarburantButton" onAction="#handleAjouterCarburantButton" styleClass="chooseImageButton" stylesheets="@../windowsSwitcher/windowsSwitcher.css" text="Ajouter" />
               </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="57.0" prefHeight="47.0" prefWidth="500.0">
               <padding>
                  <Insets top="20.0" />
               </padding>
               <children>
                  <Label prefWidth="160.0" stylesheets="@../Css/MinitextStyle.css" text="Marque :">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <JFXComboBox fx:id="marqueComboBox" focusColor="#63b4ff" layoutX="180.0" layoutY="30.0" onAction="#TypeOfTheMarque" prefHeight="25.0" prefWidth="180.0" promptText="Marque" style="-jfx-label-float: true;" unFocusColor="#cdcdcd">
                     <stylesheets>
                        <URL value="@../Css/combo.css" />
                        <URL value="@../Css/MinitextStyle.css" />
                     </stylesheets>
                     <HBox.margin>
                        <Insets left="60.0" />
                     </HBox.margin>
                  </JFXComboBox>
                  <JFXButton fx:id="ajouterMarqueButton" onAction="#handleAjouterMarqueButton" styleClass="chooseImageButton" stylesheets="@../windowsSwitcher/windowsSwitcher.css" text="Ajouter" />
               </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="196.0" prefHeight="47.0" prefWidth="500.0">
               <padding>
                  <Insets top="20.0" />
               </padding>
               <children>
                  <Label prefWidth="160.0" stylesheets="@../Css/MinitextStyle.css" text="Type :">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <JFXComboBox fx:id="typeComboBox" disable="true" focusColor="#63b4ff" prefHeight="25.0" prefWidth="180.0" promptText="Type" style="-jfx-label-float: true;" unFocusColor="#cdcdcd">
                     <stylesheets>
                        <URL value="@../Css/combo.css" />
                        <URL value="@../Css/MinitextStyle.css" />
                     </stylesheets>
                     <HBox.margin>
                        <Insets left="60.0" />
                     </HBox.margin>
                  </JFXComboBox>
                  <JFXButton fx:id="ajouterTypeButton" disable="true" onAction="#handleAjouterTypeButton" styleClass="chooseImageButton" stylesheets="@../windowsSwitcher/windowsSwitcher.css" text="Ajouter" />
               </children>
            </HBox>
            <HBox alignment="BOTTOM_LEFT" layoutX="10.0" layoutY="198.0" prefHeight="47.0" prefWidth="500.0">
               <children>
                  <Label prefHeight="29.0" prefWidth="220.0" stylesheets="@../Css/MinitextStyle.css" text="Couleur :">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <ColorPicker fx:id="colorColorPicker" prefHeight="29.0" prefWidth="180.0" styleClass="colorPicker" stylesheets="@../windowsSwitcher/windowsSwitcher.css" />
               </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="151.0" prefHeight="47.0" prefWidth="500.0" spacing="5.0">
               <padding>
                  <Insets top="20.0" />
               </padding>
               <children>
                  <Label prefHeight="17.0" prefWidth="214.0" stylesheets="@../Css/MinitextStyle.css" text="Parking :">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <JFXComboBox fx:id="parkingComboBox" focusColor="#63b4ff" prefHeight="25.0" prefWidth="180.0" promptText="Parking" style="-jfx-label-float: true;" unFocusColor="#cdcdcd">
                     <stylesheets>
                        <URL value="@../Css/combo.css" />
                        <URL value="@../Css/MinitextStyle.css" />
                     </stylesheets>
                  </JFXComboBox>
                  <JFXButton fx:id="ajouterParkingButton" onAction="#handleAjouterParkingButton" styleClass="chooseImageButton" stylesheets="@../windowsSwitcher/windowsSwitcher.css" text="Ajouter" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <JFXButton fx:id="closeButton" layoutX="10.0" layoutY="10.0" onAction="#closeButtonAction" prefHeight="40.0" prefWidth="120.0" style="-fx-background-color: #E53434; -fx-background-radius: 40;" stylesheets="@../Css/MinitextStyle.css" text="Annuler" textFill="WHITE" />
                  <JFXButton fx:id="submitButton" onAction="#handleSubmitButton" prefHeight="40.0" prefWidth="140.0" style="-fx-background-color: #1ED760; -fx-background-radius: 40;" stylesheets="@../Css/MinitextStyle.css" text="Enregistrer" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="100.0" />
                     </HBox.margin>
                  </JFXButton>
               </children>
            </HBox>
            <Label fx:id="erreurMessage" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="534.0" text="Remplissez tous les champs !!" textFill="RED" visible="false" />
         </children>
          </VBox>
    </children>

</AnchorPane>
